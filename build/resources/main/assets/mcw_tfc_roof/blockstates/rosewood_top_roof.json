{
  "multipart": [

	{ 
	  "when": { "part": "pyramid" },
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_pyramid"}
	},

    {
      "when": { "AND": [  {"part": "basic|end_part","north": "true","west": "true","east": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_outer"}
    },
	{
      "when": { "AND": [  {"part": "basic|end_part","north": "true","east": "true","west": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_outer", "y": 90}
    },
	{
      "when": { "AND": [  {"part": "basic|end_part","south": "true","east": "true","west": "false","north": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_outer", "y": 180}
    },
	{
      "when": { "AND": [  {"part": "basic|end_part","south": "true","west": "true","east": "false","north": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_outer", "y": 270}
    },	
		
	{
      "when": { "AND": [  {"part": "basic|end_part","north": "true", "east": "true","south": "false", "west": "true"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_three_way_roof", "y": 270}
    },		
	{
      "when": { "AND": [  {"part": "basic|end_part","north": "false", "east": "true","south": "true", "west": "true"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_three_way_roof", "y": 90}
    },	
	{
      "when": { "AND": [  {"part": "basic|end_part","north": "true", "east": "true","south": "true", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_three_way_roof"}
    },	
	{
      "when": { "AND": [  {"part": "basic|end_part","north": "true", "east": "false","south": "true", "west": "true"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_three_way_roof", "y": 180}
    },	

    {
      "when": { "AND": [  {"part": "basic", "north": "false", "south": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof"}
    },
    {
      "when": { "AND": [  {"part": "basic","south": "false","north": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof"}
    },
    {
      "when": { "AND": [  {"part": "basic","east": "false","west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof", "y": 90}
    },
  	{
      "when": { "AND": [  {"part": "basic","west": "false","east": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof", "y": 90}
    },
	
	{
      "when": { "AND": [  {"part": "basic|end_part","east": "true", "west": "true", "north": "true", "south": "true"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_four_way_roof"}
    },
	
	{
      "when": { "AND": [  {"part": "basic","east": "true", "west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof", "y": 90}
    },
	{
      "when": { "AND": [  {"part": "basic","south": "true", "north": "true", "east": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof"}
    },
	{
      "when": { "AND": [  {"part": "basic","south": "false", "east": "false", "north": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof"}
    },
		
    {
      "when": { "AND": [  {"part": "end_part", "north": "false", "south": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end", "y": 180}
    },
    {
      "when": { "AND": [  {"part": "end_part","south": "false","north": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end"}
    },
    {
      "when": { "AND": [  {"part": "end_part","east": "false","west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end", "y": 270}
    },
  	{
      "when": { "AND": [  {"part": "end_part","west": "false","east": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end", "y": 90}
    },

	{
      "when": { "AND": [  {"part": "end_part","east": "true", "west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end", "y": 90}
    },
	{
      "when": { "AND": [  {"part": "end_part","south": "true", "north": "true", "east": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end"}
    },
	{
      "when": { "AND": [  {"part": "end_part","south": "false", "east": "false", "north": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_roof:block/top/rosewood_top_roof_end"}
    }

  ]
}